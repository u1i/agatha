# Agatha Multi-Model AI Consensus System Environment Variables

# The model for the agent and orchestration
CHAT_MODEL=gemini-2.5-pro
CHAT_MODEL_TEMPERATURE=0.7
CHAT_MODEL_PROVIDER=openrouter

# Step 1 Model: Grounding
MODEL_STEP1=gemini-2.5-flash
TEMPERATURE_STEP1=0.3
MODEL_STEP1_PROVIDER=openrouter

# Step 2 Model: Deep Analysis
MODEL_STEP2=anthropic/claude-opus-4
TEMPERATURE_STEP2=0.7
MODEL_STEP2_PROVIDER=openrouter

# Step 3 Model: Fact-check and Proofread
MODEL_STEP3=openai/o4-mini
TEMPERATURE_STEP3=0.3
MODEL_STEP3_PROVIDER=openrouter

# Step 4 Model: Synthesis and Consensus
MODEL_STEP4=gemini-2.5-pro
TEMPERATURE_STEP4=0.5
MODEL_STEP4_PROVIDER=openrouter

# API Keys (fill in your actual keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Direct API keys if not using OpenRouter
# GOOGLE_API_KEY=your_google_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# OPENAI_API_KEY=your_openai_api_key_here

# Application Settings
PORT=3000
NODE_ENV=development
